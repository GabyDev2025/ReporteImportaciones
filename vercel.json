{
  "version": 2,
  "builds": [
    {
      "src": "api/procesar_importaciones.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb" }
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/", "dest": "/index.html" },
    { "src": "/api/(.*)", "dest": "/api/procesar_importaciones.py" }
  ]
}
